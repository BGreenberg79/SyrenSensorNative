{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"jsx": "react-native",
		"module": "esnext",
		"target": "esnext",
		"lib": ["esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"moduleResolution": "node"
	},
	"exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
